             <w lemma="filia"><expan><abbr>f</abbr><ex>ilia</ex></expan></w>  <g ref="symbols.xml#stop"/> <name nymRef="Palatina">Palatina</name></persName>
            <lb n="2"/><persName type="attested"><name nymRef="Iulius"><expan><abbr>Iul</abbr><ex>ius</ex></expan></name> <g ref="symbols.xml#stop"/>
               <name nymRef="Democritus">Democritus</name>
            <lb n="3"/> <w lemma="eques"><expan><abbr>eq</abbr><ex>ues</ex></expan></w> 
               <placeName type="ethnic" nymRef="#Romanus" ref="#roma"><expan><abbr>R</abbr><ex>omanus</ex></expan></placeName></persName> <g ref="symbols.xml#stop"/>
               <w lemma="pro">pro</w> <w lemma="salus">salute</w>
