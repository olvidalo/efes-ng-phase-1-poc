         <div type="edition" xml:lang="la" xml:space="preserve">
            <ab>
          <lb n="1"/><persName type="attested"><name nymRef="Aelia"><expan><abbr>Ael</abbr><ex>ia</ex></expan></name> <g ref="symbols.xml#stop"/>
