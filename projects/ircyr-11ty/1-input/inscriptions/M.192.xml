            <lb n="5"/><w lemma="et">et</w> <persName type="attested"><name nymRef="Iulius"><!-- see l. 4 --></name> <name nymRef="Catullinus">Catullini</name></persName> <w lemma="filius"><expan><abbr>filior</abbr><ex>um</ex></expan></w>
            <lb n="6"/><w lemma="suus">suorum</w> <g ref="symbols.xml#stop"/> <persName type="divine" key="asklepios"><name nymRef="Asculapius">Aesculapio</name></persName>
