                    <lb n="1"/> <w lemma="ὁ"><supplied reason="lost" cert="low">τῶ</supplied></w>
                        <persName type="divine" key="apollo"><name nymRef="Ἀπόλλων"><supplied reason="lost" cert="low">Ἀπόλλω</supplied><unclear>ν</unclear>ος</name></persName> <w lemma="ἱεριτεύω">ἱεριτεύων</w>
                    <lb n="2"/><gap reason="lost" quantity="8" unit="character" precision="low"/><orig><unclear>ρ</unclear></orig> <persName type="attested"><name nymRef="Φιλίννας" key="lgpn:V1-65915">Φιλίννα</name></persName>
                    <lb n="3"/><persName type="divine" key="iatros"><name nymRef="Ἰατρός"><supplied reason="lost">Ἰατρῶι</supplied></name></persName> <w lemma="καί"><supplied reason="lost">κα</supplied>ὶ</w>
                  <persName type="divine" key="iaso"><name nymRef="Ἰασώ">Ἰασῶι</name></persName>
                    <lb n="4"/> <w lemma="ὁ"><supplied reason="lost" cert="low">τὸν</supplied></w> <w lemma="βωμός"><supplied reason="lost" cert="low">βωμὸν</supplied></w> <w lemma="ἱδρύω"><supplied reason="lost">ἱ</supplied>δρύσατο</w>
                </ab>
