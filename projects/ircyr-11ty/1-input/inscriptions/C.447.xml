          <supplied reason="lost" cert="low"><w lemma="χάρις">χάριν</w></supplied> <w lemma="καί"><supplied reason="lost">κ</supplied>αὶ</w> <w lemma="ὁ">τᾶς</w> <w lemma="πρός">ποτὶ</w>	
          <lb n="3"/><supplied reason="lost"><w lemma="ὁ">τὰς</w> <w lemma="θεά">θεὰς</w></supplied> 
            <w lemma="εὐσέβεια"><supplied reason="lost">εὐ</supplied><unclear>σε</unclear>βέας</w> <space extent="unknown" unit="character"/> 
          <lb n="4"/> <placeName type="ethnic" nymRef="#Κυραναῖος" ref="#cyrene"><supplied reason="lost">Κυρ</supplied><unclear>ά</unclear>ν<unclear>α</unclear>ιοι</placeName>  
